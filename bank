def account_details(account_number, holder_name, account_type, balance):
    return (
        f"Account Number:(account_number)\n"
        f"Account Holder Name:(holder_name)\n"
        f"Account Type:(account_type)\n"
        f"Account Balance"(account_balance)
    )

if__name__=="__main__:"
